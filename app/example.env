# OpenAI API Key (optional - using Gemini instead)
# OPENAI_API_KEY=

# Google Gemini API Key (get from https://ai.google.dev/)
GEMINI_API_KEY=

MY_EMAIL=

# Email: use Resend (recommended for Render) or Gmail SMTP (local dev)
# Resend: https://resend.com - works on Render free tier
RESEND_API_KEY=
FROM_EMAIL=onboarding@resend.dev

# Gmail SMTP (optional, for local dev only - blocked on Render free tier)
APP_PASSWORD=

# Multi-user: Import subscribers from Google Sheets (published as CSV)
# Get URL from: Google Sheet → File → Share → Publish to web → CSV
SUBSCRIBERS_CSV_URL=

POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=ai_news_aggregator
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

